<script>
function mostrarLoguinCab(){
	document.getElementById('ventana-ingresar').style.display = 'block';
}

function ocultarLoguinCab(){
	document.getElementById('ventana-ingresar').style.display = 'none';
}

function mostrarCapa(capa){
	document.getElementById(capa).style.display = 'block';
}

function ocultarCapa(capa){
	document.getElementById(capa).style.display = 'none';

}
</script>

<script>
function busqueda2(forma)
{
var i,valorano,valormes,valordia,anominimo,mesminimo,diaminimo;
if ((forma.ano.selectedIndex)&&(forma.mes.selectedIndex)&&(forma.dia.selectedIndex)) {
anominimo = 2005;
mesminimo = 0;
diaminimo = 2;
i=forma.ano.selectedIndex;
valorano=forma.ano.options[i].value;
i=forma.mes.selectedIndex;
mesnumero = i-1;
valormes=forma.mes.options[i].value;

i=forma.dia.selectedIndex;
dianumero = i;
valordia=forma.dia.options[i].value;

fecha_minima = new Date (anominimo,mesminimo,diaminimo);
fecha_usuario = new Date (valorano,mesnumero,dianumero);
fecha_hoy = new Date();
//alert("minima: "+fecha_minima+"   usuario: "+fecha_usuario+"   hoy: "+fecha_hoy);
if ((fecha_usuario < fecha_minima)||(fecha_usuario > fecha_hoy)) {
alert("Su selecci�n no esta disponible en nuestro historico, Por favor seleccione una fecha diferente");
return;
}
else {
 switch(valormes){
      case 'ene':
	     mes=1;
	  break;
	  case 'feb':
	     mes=2;
	  break;	  
	  case 'mar':
	     mes=3;
	  break;
	  case 'abr':
	     mes=4;
	  break;
	  case 'may':
	     mes=5;
	  break;
	  case 'jun':
	     mes=6;
	  break;
	  case 'jul':
	     mes=7;
	  break;
	  case 'ago':
	     mes=8;
	  break;
	  case 'sep':
	     mes=9;
	  break;
	  case 'oct':
	     mes=10;
	  break;
	  case 'nov':
	     mes=11;
	  break;
	  case 'dic':
	     mes=12;
	  break;
	  
 }
 
fecha=valorano+"-"+mes+ "-" + valordia;
//if(fecha=="ene012010")valordia="01";

var f1 =  new Date(2010, 5, 12);
var f2 =  new Date(valorano,mes-1,valordia);
if(CompareDates(f1,f2)){
        url= "/elpais/ediciones.php?p=" + valormes + valordia + valorano;
     
}
	 else{
	 url= "/elpais/historico/historico-pais?fecha=" + fecha;
   
    }

window.open(url,"_blank");


return;
}
}
else alert("Por favor seleccione una fecha valida. Gracias");
}
 
function CompareDates(fecha1,fecha2){
    if(fecha1 <= fecha2){
        return false;
    }
	
	return true;
} 


function paginaInicio(){

  if ((navigator.appName=="Microsoft Internet Explorer") && (parseInt(navigator.appVersion)>=4)) { 
    var url=" http://www.elpais.com.co "; 
    var titulo="ELPAIS.COM.CO"; 
     window.external.AddFavorite(url,titulo); 
  } else { 
     if(navigator.appName == "Netscape") 
     alert (" Presiona Crtl+D para agregar ELPAIS.COM.CO a tus Favoritos "); 
    }


	
	
}

</script>

