<script type="text/javascript">
	function mostrar(capa){
		document.getElementById('abrirMapa').style.visibility  = 'hidden';
		document.getElementById('cerrarMapa').style.visibility = 'visible';
		$("#"+capa).show("slow");	
	}
	
	function ocultar(capa){
		document.getElementById('cerrarMapa').style.visibility = 'hidden';
		document.getElementById('abrirMapa').style.visibility  = 'visible';
		$("#"+capa).hide("slow");
	}
</script>
