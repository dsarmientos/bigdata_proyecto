    function IEFunctionality()
    {
	    try{
        if (window.external.msIsSiteMode())
        {         
           if(contadorFunc == 0){
            //omniture desde el alclaje de PIN IE9
            var s=s_gi('eltiempocom'); 
            s.linkTrackVars='eVar33,events'; 
            s.linkTrackEvents='event26'; 
            s.eVar33='Tracking IE9'; 
            s.events='event26';  
            s.tl(this,'o','Tracking IE9'); 
            
            s.prop43 = "PIN IE 9";  
		    var s_code=s.t();if(s_code)document.write(s_code);//recuest
		   }
		   contadorFunc++;
            /*ATENCION OMITIR ESTA LINEA AL PONER EL SITE EN PRODUCCION*/
            //jQuery.support.cors = true;
            //var counter = 1;	
		    $.ajax(
		    {
			    type: "GET",
			    url: "http://www.eltiempo.com/pinie9_rss.xml",
			    dataType: "xml",
			    success: function(xml) 
			    {
                    window.external.msSiteModeClearJumpList();
                    window.external.msSiteModeCreateJumpList('Secciones de interés');
                    
                    var counterItem = 0;    
				    var str="";
				    $(xml).find('item').each(function()
				    {
                        var title = $(this).find('title').text();
                        var link = $(this).find('link').text();
                        //adicionar icono de notificacion   /media/images/favicons/favicon_etalerta.ico
                        //se puede cambiar el icono por el numero respectivo por ejemplo
                        
                        if(title != 'no hay streaming en eltiempo.com' && counterItem == 0){
                            //window.external.msSiteModeClearIconOverlay();
                            window.external.msSiteModeSetIconOverlay("http://www.eltiempo.com/media/images/favicons/faviconetalerta.ico");
                            window.external.msSiteModeActivate();
                            //window.external.msSiteModeSetIconOverlay("http://www.eltiempo.com/media/images/favicons/favicon_etalerta.ico", title);
                        }else{
                        //media/images/favicons/favicon_et.ico
                            //window.external.msSiteModeSetIconOverlay("http://www.eltiempo.com/media/images/favicons/favicon_et.ico", title);
                            if(title == 'no hay streaming en eltiempo.com' && counterItem == 0){
                                window.external.msSiteModeClearIconOverlay();
                            }
                        }

                        //Adicionar noticia a las 5 noticias recomendadas
                        //se puede cambiar el icono por el numero respectivo por ejemplo
                        if(counterItem > 0)
                            window.external.msSiteModeAddJumpListItem(title, link, "http://www.eltiempo.com/favicon.ico");

                        counterItem++;
				    }); 
				    window.external.msSiteModeShowJumpList();
			    },
                error:function(jqXHR, textStatus, errorThrown)
                {}
		    });
  
	    }
	    }catch(e){
            //console.log("- Error " + e)    
        }
    }
