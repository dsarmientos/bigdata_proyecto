<script type="text/javascript">
		var hoy = new Date();	
		var ufp='http://www.eltiempo.com/pauta/pautafondo.php?sec=cat_politica&date='+hoy.getFullYear()+'-'+(hoy.getMonth()+1)+'-'+hoy.getDate();
$.ajax({
        url: ufp,
        async:true,
        cache:true,
        contentType: "application/x-www-form-urlencoded",
        dataType: "html",
        success: function(datos){
			document.body.style.background=datos;
		/*	if(datos.length>0){
				document.body.style.backgroundAttachment="fixed";			
			}*/
        },
        timeout: 3000,
        type: "GET"
});
</script>  

       
