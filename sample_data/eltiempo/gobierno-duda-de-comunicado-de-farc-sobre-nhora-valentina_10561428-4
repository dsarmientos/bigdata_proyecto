<script>
function mostrarOcultarSecciones(){
    if($('#listadesecciones').hasClass('activo')){
        $('#listadesecciones').removeClass('activo');
	  //$('.subMenuSecciones').hide('slow');
    }else{
        $('#listadesecciones').addClass('activo');
	  //$('.subMenuSecciones').show('slow');
    }
}
  
  $('#listadesecciones').bind('click', function(event){
	  if(!event.preventDefault){ 
          event.preventDefault();
      }else{
          event.returnValue = false;
      }
	  $('.subMenuSecciones').animate({
          height: "toggle"
		
      });
  });
</script>
                                                                                                                    
<script type="text/javascript">
$(document).ready(function(data){
   $('.clasemouseenter').mouseenter(function(data){
      $('#'+this.id+'1').show();
   }).mouseleave(function(data){
         var rel = data.relatedTarget;
         var cur = data.currentTarget;
         if(rel.tagName){
            $('#'+this.id+'1').hide();
         }
   });        
});
</script>

