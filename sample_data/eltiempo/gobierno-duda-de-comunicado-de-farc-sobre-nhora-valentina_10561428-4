   $('#pypCiudad1').load('/servicios/pico_placa/'+aliasciudad+'.html');
   if(aliasciudad!='bogota' && aliasciudad!='barranquilla' && aliasciudad!='cali' && aliasciudad!='medellin'){
      aliasciudadclima = 'bogota';
   }else{
      aliasciudadclima = aliasciudad;
   }
   
   //probar en caliente
	//		   if(aliasciudad=='bogota'){
			      //bogota
		//	      $('#climaCiudad1').load('http://'.$IP_CMS.'/interfaz/paginas/ver_pagina.php?id_pagina=6578905');
			//   }else  
			//    if(aliasciudad=='barranquilla'){
			       //bquilla
 		     //      $('#climaCiudad1').load('http://'.$IP_CMS.'/interfaz/paginas/ver_pagina.php?id_pagina=6605065');
			 //   }else
			  //   if(aliasciudad=='cali'){
			        //cali
