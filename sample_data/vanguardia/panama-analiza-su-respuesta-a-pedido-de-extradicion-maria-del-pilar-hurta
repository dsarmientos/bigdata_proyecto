			$("#solovl").html("<img src='http://www.vanguardia.com/sites/default/files/img/com_van.png' alt='Agregar comentario' title='Agregar comentario' />");
			$("#soloface").html("<img src='http://www.vanguardia.com/sites/default/files/img/com_face2.png' alt='Agregar comentario' title='Agregar comentario' />");
		});
		$("#soloface").click(function(){
			$("#comments").css("display","none");
			$("#commentsface").css("display","block");
			$("#solovl").html("<img src='http://www.vanguardia.com/sites/default/files/img/com_van2.png' alt='Agregar comentario' title='Agregar comentario' />");
			$("#soloface").html("<img src='http://www.vanguardia.com/sites/default/files/img/com_face.png' alt='Agregar comentario' title='Agregar comentario' />");
		});
	</script>
</div></div>
        </div>
