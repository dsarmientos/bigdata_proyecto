function cambiaiab(modo)
{
	if (modo==1)
		document.getElementById('imgiab').src="/sites/default/files/img/logo_iab_c.png";
	else
		document.getElementById('imgiab').src="/sites/default/files/img/logo_iab_g.png";
}

function cambiacert(modo)
{
	if (modo==1)
		document.getElementById('imgcertifica').src="/sites/default/files/img/certifica_c.png";
	else
		document.getElementById('imgcertifica').src="/sites/default/files/img/certifica_g.png";
}

function cambiapera(modo)
{
	if (modo==1)
		document.getElementById('imgpera').src="/sites/default/files/img/pera_c.png";
	else
		document.getElementById('imgpera').src="/sites/default/files/img/pera_g.png";
}
</script>
