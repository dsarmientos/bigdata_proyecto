	function pagerFactory_video(idx) {

		link = '<im'+'g  width="55" style="cursor:pointer;" src="'+get_imagen_video(idx)+'" class="thumb_multimedia">';
		var s = idx > 6 ? ' style="display:none"' : ' '; 
		return '<li onclick="cambiar_imagen_video('+idx+')">'+link+'</li>'; 
	};
	
   function get_imagen_video(id){
	return lista_videos[id];
	};
	
	function cambiar_imagen_video(index){
	$("#bloque_der_int_59 .bloque_contenido .view-content").fadeOut("slow", function() {
	$("#bloque_der_int_59 .bloque_contenido .view-content").html(img_originales_video[index]);
	$("#bloque_der_int_59 .bloque_contenido .view-content").fadeIn("slow");
	 });
