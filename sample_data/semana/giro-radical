// Script para agragar a Favoritos
    function Agregar_Favoritos()
    {
        if ((navigator.appName == "Microsoft Internet Explorer") && (parseInt(navigator.appVersion)>3))
        {
            var url="http://www.semana.com/";
            var titulo="Semana.com, Análisis de actualidad";
            window.external.AddFavorite(url,titulo);
        }
        else 
        {
            //Para Firefox
            if(navigator.appName == "Netscape")
            {
                window.sidebar.addPanel("Semana.com, Análisis de actualidad", "http://www.semana.com/","");
            }
        }
    }
    
    // Script para establecer como página de inicio
    function Pagina_Inicio()
    {
        if ((navigator.appName == "Microsoft Internet Explorer") && (parseInt(navigator.appVersion)>3))
        {
            obj_Pagina_Inicio.style.behavior='url(#default#homepage)';
            obj_Pagina_Inicio.setHomePage("http://www.semana.com");
        }
        else
        {
            alert("Su navegador no dispone de esta opción.");
        }
    } 
</script>

