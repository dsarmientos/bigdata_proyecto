<script type="text/javascript">
	
		
		function idArticulo() {
            var idArt;
            var vars = [], hash;
            //var hashes = window.location.href.slice(window.location.href.indexOf('?') + 1).split('&');
			var hashes = location.href.split('/');
			for (var i = 0; i < hashes.length; i++) {
                hash = hashes[i].split('-3');
				if (hash.length == 2)
				{
					if (hash[1] == ".aspx") {
						idArt = hash[0];
						break;
					}
				}
            }
            return idArt;
        }
		
		function verificarIds() {
            var identificadorArt = idArticulo();
			if (identificadorArt == '163813' || identificadorArt == '146753' || identificadorArt == '166448' || identificadorArt == '127515' || identificadorArt == '162697' || identificadorArt == '181248'){
				div = document.getElementById('divFacebook');
				div.style.display='';
			}
        }

        
		function registrarActividad()
		{
			//Verificar si viene de la funcionalidad leer de facebook
			var referrer_url = document.referrer;
			if (referrer_url != '' && referrer_url != null)
			{
				if (referrer_url.indexOf("www.facebook.com") !=-1 && referrer_url.indexOf("revistasemana") !=-1) {
					//Adicionar actividad de semana en facebook
					postArticle(true);
				}
			}
		}
		  
		var varCounter = 0;
		var refreshIntervalId;
		  
		   
		function verificarActividad()
		{
			if(varCounter <= 10) {
				  varCounter++;
				  iniciarFacebook();
				  postArticle(false);
			 } else {
				  clearInterval(refreshIntervalId);
			 }
		}
		  
		  function postArticle(bLogin)
		  {
				document.getElementById('imgPostArticle').src = 'img/iconos/actividad_semana_1.png';
				//Llamar evento registro aplicación
				FB.api(
					'/me/revistasemana:consult',
					'post',
					{ semana: 'http://www.semana.com/nacion/caso-santoyo-hoy-semana-vivo/183058-3.aspx' },
					function(responApi) {
						if (!responApi || responApi.error) {
							if (bLogin) {
								FB.getLoginStatus(function (responseVerLogin) {
									if (!responseVerLogin.session) {
										//Mostrar ventana de autenticación
										var ventana = 'https://www.facebook.com/dialog/plugin.perms?display=popup&app_id=125335964150697&perms=publish_actions&secure=true&social_plugin=login_button&return_params=%7B%22scope%22%3A%22publish_actions%22%2C%22show_faces%22%3A%22true%22%2C%22max_rows%22%3A%221%22%2C%22width%22%3A%22200%22%2C%22app_id%22%3A%22125335964150697%22%2C%22locale%22%3A%22en_US%22%2C%22sdk%22%3A%22joey%22%2C%22channel%22%3A%22http%3A%2F%2Fstatic.ak.facebook.com%2Fconnect%2Fxd_arbiter.php%3Fversion%3D11%23cb%3Df24ea449e50d8fa%26origin%3Dhttp%253A%252F%252Fwww.semana.com%252Ff13fccaa1c143a4%26domain%3Dwww.semana.com%26relation%3Dparent.parent%22%2C%22ret%22%3A%22perms%22%7D&login_params=%7B%7D'
										window.open(ventana,'Semana en Facebook','width=400, height=300');
									}
								});
								//Inicializar verificación actividad actual
								refreshIntervalId = setInterval("verificarActividad()", 5000);
							}
						} else {
							iniciarFacebook();
							clearInterval(refreshIntervalId);
							document.getElementById('imgPostArticle').src = 'img/iconos/actividad_semana_2.png';
						}
					});
			}
			
			function iniciarFacebook()
			{
				FB.init({
					appId      : '125335964150697', // App ID
					status     : true, // check login status
					cookie     : true, // enable cookies to allow the server to access the session
					xfbml      : true  // parse XFBML
				  });
			}
			
</script>

