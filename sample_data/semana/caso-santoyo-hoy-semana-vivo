<script type="text/javascript" language="javascript">
function uptext(){
	ts('titular_articulo',1);ts('sumario_articulo',1);ts('texto2',1);ts('texto1',0);
}
function downtext(){
	ts('titular_articulo',-1);ts('sumario_articulo',-1);ts('texto2',-1);ts('texto1',0);
}
function print(){    
    window.open('http://www.semana.com//wf_ImprimirArticulo.aspx?IdArt=183058'); 
}
function save(){
    hs.htmlExpand(document.getElementById('asave') , {src: 'wf_GuardarArticulo.aspx?IdRef=183058&IdTab=1', objectType: 'iframe', outlineType: 'rounded-white', preserveContent: false, objectHeight: 310  } )           
}
function send1(){
    hs.htmlExpand(document.getElementById('asend') , {src: 'wf_Enviar.aspx?IdRef=183058&IdTab=1', objectType: 'iframe', outlineType: 'rounded-white', preserveContent: false, objectHeight: 310  } )        
}
function comment(){
    hs.htmlExpand(document.getElementById('acoment') , {src: 'wf_Comentar.aspx?IdRef=183058&IdTab=1', objectType: 'iframe', outlineType: 'rounded-white', preserveContent: false, objectHeight: 470  } )        
}
function shared(){
    hs.htmlExpand(document.getElementById('ashared') , {src: 'wf_Compartir.aspx?url=' + String(window.location).replace(/\#.*$/, "") + '&title=' + document.title, objectType: 'iframe', outlineType: 'rounded-white', preserveContent: false, objectHeight: 200  } )            
}
function rediretCaricatura(lst){
    window.location = 'http://www.semana.com//' + lst.options[lst.selectedIndex].value;
}
function favorito(){
    hs.htmlExpand(document.getElementById('afavo') , {src: 'wf_LstArticulosFavoritos.aspx' , objectType: 'iframe', outlineType: 'rounded-white', preserveContent: false, objectHeight: 200  } )            
}
function expandiframe(link){
    hs.htmlExpand(link , {src: link.src , objectType: 'iframe', outlineType: 'rounded-white', preserveContent: false  } )            
    return false;
}
</script>

