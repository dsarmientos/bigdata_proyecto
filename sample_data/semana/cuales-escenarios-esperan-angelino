    //hs.htmlExpand(document.getElementById('acom') , {src: 'http://comunidades.semana.com/wf_Login.aspx?IdRevista=1&Url=http://www.semana.com/wf_Registro.aspx?login=1', objectType: 'iframe', outlineType: 'rounded-white', preserveContent: false, objectHeight: 240, width:260  } )        
