<script type="text/javascript" language="javascript">
function uptext(){
	ts('titular_articulo',1);ts('sumario_articulo',1);ts('texto2',1);ts('texto1',0);
}
function downtext(){
	ts('titular_articulo',-1);ts('sumario_articulo',-1);ts('texto2',-1);ts('texto1',0);
}
function print(){    
    window.open('http://www.semana.com//wf_ImprimirArticulo.aspx?IdArt=186877'); 
}
function save(){
