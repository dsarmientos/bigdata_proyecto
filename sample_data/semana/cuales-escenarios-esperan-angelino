<script>

    var idsActuales = '';
	var idCookies = '';
	var mostrar = true;
	
    //Ejecutar búsqueda cada # milisegundos
    setInterval("lstNotificaciones()", 240000);
    
    //Buscar artículos y mostrar notificaciones
    function lstNotificaciones() {
          $.ajax({
              type: "POST",
			  url: "ws/ws_Busquedas.asmx/lst_NotificacionUltimosCreados",
              data: "{}",
              contentType: "application/json; charset=utf-8",
              dataType: "json",
              success: function(response) {
					var items = (new Function("return " + response))();
					if (items.length > 0) 
                    {
                        idsActuales = '';
						for (i=items.length-1; i>=0; i--)
                        {
                            mostrar = true;
							//Leer cookies id's mostrador
							if (readCookie('IdsItem') != '')
								idCookies = readCookie('IdsItem').split(',');
							
							//Verificar fecha de publicacion
							for (k=0; k<idCookies.length; k++)
							{
								if (idCookies[k] == items[i].Id)
								{
									mostrar = false;
									k = idCookies.length;
								}
							}
							
							//Pintar item si no esta registrado en la cookie
							if (mostrar)
							{
								//Verificar imagen pequenia
								if (items[i].Imagen != 'undefined' || items[i].Imagen != null)
								{
									if (items[i].AnteTitulo == 'Artículo')
										Imagen = '<img src="' + items[i].Imagen + '" width="75px" border="0" />';
									else if (items[i].AnteTitulo == 'Galería')
										Imagen = '<img src="photos/' + items[i].Imagen + '" width="75px" border="0" />';
									else
										Imagen = '<img src="http://images.semana.com/multimedia/md_' + items[i].IdArticulo + '/' + items[i].Imagen + '" width="75px" border="0" />';
								} 
								else
									Imagen = '';
								//Actualizar fecha
								fecha = items[i].FechaPublicacion    
								//Pintar html notificaciones    
									var notice = '<div class="notice">'
										  + '<div class="notice-body">' 
											  + Imagen
											  + '<h3><a href="' + items[i].Url + '" style="color: #000; text-decoration: none; font-family:Georgia,Times New Roman,Times,serif;">' + items[i].Titulo + '</a></h3>'
											  + '<h3 style="text-align:center;"><a href="' + items[i].Url + '" style="color: #C80202; font-size:13px; text-decoration: none; font-family:Arial,Helvetica,sans-serif;">Ver ' + items[i].AnteTitulo + '</a></h3>'
										+ '</div>'
										+ '<div class="notice-bottom"></div>'
									+ '</div>';
					
								//Mostrar notificación
								$( notice ).purr(
									{
										usingTransparentPNG: true
									}
								);
							}
							//Actualizar id's cookies
							if (idsActuales == '')
								idsActuales = items[i].Id;
							else
								idsActuales += ',' + items[i].Id;
						}
						//Actulizar cookie con última fecha
						createCookie('IdsItem', idsActuales);
							return false;
					}
              },
              error: function(result) {
                  alert('ERROR ' + result.status + ' ' + result.statusText);
              }
          });
      }
    
    function pad(number, length) 
    {
		var str = '' + number;
		while (str.length < length) {
			str = '0' + str;
		}
		return str;
    }
    
    function datosPrueba() 
    {
		eraseCookie('IdsItem');
		createCookie('IdsItem', '');
		alert(readCookie('IdsItem'));
	}
   
   //Inicializar notificaciones
   $(function() {
        setTimeout("lstNotificaciones()", 10000);
	});
    

</script>
<div style="display:none;">
	<input type="button" onclick="eraseCookie('IdsItem');" value="borrar cookie" name="borrar">
	<input type="button" onclick="lstNotificaciones();" value="inicializar cookie" name="inicializar">
	<input type="button" onclick="alert(readCookie('IdsItem'));" value="leer cookie" name="leer">
</div>	


